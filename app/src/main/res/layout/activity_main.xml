<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    tools:context="cn.edu.gdmec.s07150829.intent.MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="显示启动activity"
        android:onClick="componentname"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="隐藏启动activity"
        android:onClick="intentfilter"/>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="http://www.gdmec.com"
        android:id="@+id/url"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打开网页"
        android:onClick="view"/>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="120"
        android:id="@+id/phone"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="拨号"
        android:onClick="dial"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="转号"
        android:onClick="forresult"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview1"
        android:text="等待转换"
        />
</LinearLayout>
